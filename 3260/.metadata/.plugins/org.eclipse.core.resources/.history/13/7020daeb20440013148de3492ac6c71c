/**
 * Main class to load the board
 * @author trevor
 * 
 */
public class Frogger {

    public static void main(String[] args) {
    	Input in = new Input();
        Window win = new Window("Frogger", 500,500);
        Map map = new Map(9,9,win);
        win.addKeyListener(in);
        win.addToDrawList(map);
        win.repaint();
        
    }
}